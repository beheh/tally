{
  "compilerOptions": {
    "declaration": false,
    "target": "es5",
    "lib": [
      "es6",
      "dom"
    ],
    "moduleResolution": "node",
    "isolatedModules": false,
    "jsx": "react",
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "suppressImplicitAnyIndexErrors": true,
    "strictNullChecks": true,
    "sourceMap": false,
    "outDir": "build/"
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
